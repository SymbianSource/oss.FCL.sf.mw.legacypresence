        {
        INTERFACE_INFO
            {
            // UID of interface that is implemented
            interface_uid = IM_IF_ID_PROTOCOL_IM_FEATURES;
            implementations = 
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid = KImplUidSimpleImPlugin;
                    version_no = 1;
                    display_name = "simple im plugin";
                    default_data = 2219;
                    opaque_data = "";
                    rom_only = 0;
                    }
                };
            }
        };
    }

